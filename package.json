    "@vue/typescript": "^1.8.20",
    "compression-webpack-plugin": "^11.1.0",
    "autoprefixer": "^10.4.8",
    "babel-eslint": "^10.1.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.32.0",
    "husky": "^9.1.7",
    "jscpd": "^4.0.5",
    "typescript": "~4.7.4",
    "lint-staged": "^15.2.10",
