{
  "name": "front-mobile-vue3",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://github.com/poohlaha/front-mobile-vue3.git",
  "engines": {
    "node": ">=16.0.0"
  },
  "description": "`Vue3` `手机` 端框架.",
  "keywords": [
    "vue",
    "css",
    "js"
  ],
  "author": "poohlaha",
  "license": "MIT/Apache-2.0",
  "scripts": {
    "prepare": "husky install",
    "copy": "node ./scripts/index.js copy",
    "start": "node ./scripts/index.js start",
    "build": "node ./scripts/index.js dev",
    "prod": "node ./scripts/index.js prod",
    "simulate": "node ./scripts/index.js simulate",
    "format": "npx prettier --write src",
    "report": "jscpd --pattern 'src/views/**/*.{vue,ts,js}'",
    "updater": "node ./scripts/updater.js"
  },
  "dependencies": {
    "@bale-web/request": "^0.2.0",
    "@bale-wasm/http": "^0.1.1",
    "fastclick": "^1.0.6",
    "typescript": "^5.7.2",
    "js-base64": "^3.7.7",
    "pinia": "^2.3.0",
    "tailwindcss": "^3.4.17",
    "sm-crypto": "^0.3.12"
  },
  "devDependencies": {
    "@bale-tools/utils": "^2.0.8",
    "@bale-tools/mutate-minimize": "^1.0.7",
    "@bale-tools/mutate-service": "^2.4.1",
    "@bale-tools/mutate-version": "^2.3.9",
    "core-js": "^3.8.3",
    "vue-loader": "^17.4.2",
    "@babel/eslint-parser": "^7.25.9",
    "@commitlint/cli": "^19.7.1",
    "@vue/compiler-sfc": "^3.5.13",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.4",
    "@vue/babel-plugin-jsx": "^1.2.5",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-router": "~5.0.8",
    "@vue/typescript": "^1.8.20",
    "compression-webpack-plugin": "^11.1.0",
    "autoprefixer": "^10.4.8",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.32.0",
    "husky": "^9.1.7",
    "jscpd": "^4.0.5",
    "lint-staged": "^15.2.10",
    "n-readlines": "^1.0.1",
    "postcss-aspect-ratio-mini": "^1.1.0",
    "postcss-write-svg": "^3.0.1",
    "toml": "^3.0.0",
    "vconsole": "^3.15.1",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^3.5.2",
    "workbox-webpack-plugin": "^7.3.0",
    "@babel/preset-typescript": "^7.26.0",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "@vue/cli-plugin-typescript": "~5.0.8",
    "eslint-plugin-unused-imports": "^4.1.4",
    "useless-files-webpack5-plugin": "^1.0.2",
    "postcss-import": "^16.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{vue,jsx,js,tsx,ts,json,css,md}": [
      "prettier --write"
    ],
    "src/**/*.{vue,jsx,js,tsx,ts}": [
      "eslint --fix"
    ]
  }
}
